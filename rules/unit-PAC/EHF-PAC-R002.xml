				xmlns="urn:oasis:names:specification:ubl:schema:xsd:Catalogue-2">

			<cac:ProviderParty>
				<cac:PartyIdentification>
					<cbc:ID schemeID="0088">5790000435951</cbc:ID>
				</cac:PartyIdentification>
			</cac:ProviderParty>

