<?xml version="1.0" encoding="UTF-8"?>
<Syntax xmlns="urn:fdc:difi.no:2017:vefa:structure:Syntax-1">

    <Term>PreAwardCatalogueRequest</Term>
    <Name>PreAward Catalogue Request</Name>

    <Namespace prefix="ubl">urn:oasis:names:specification:ubl:schema:xsd:CatalogueRequest-2</Namespace>
    <Namespace prefix="cac">urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2</Namespace>
    <Namespace prefix="cbc">urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2</Namespace>

    <Document>
        <Term>ubl:Catalogue</Term>

        <Element>
            <Term>cbc:UBLVersionID</Term>
            <Name>UBL version identifier</Name>
            <Description>Identifies the version of syntax that transactions are based on.</Description>
            <Value type="FIXED">2.2</Value>
        </Element>

        <Element>
            <Term>cbc:CustomizationID</Term>
            <Name>Customization identifier</Name>
            <Description>Identifies the specification of content and rules that apply to the transaction.</Description>
            <Value type="FIXED">urn:fdc:difi.no:2017:ehf:CatalogueRequest:2.0</Value> <!-- mÅ SJEKKES OPP!-->
        </Element>

        <Element>
            <Term>cbc:ProfileID</Term>
            <Name>Profile identifier</Name>
            <Description>Identifies the business process context in which the transaction appears.</Description>
            <Value type="FIXED">urn:fdc:difi.no:2017:ehf:profile:02:1.0</Value> <!-- mÅ SJEKKES OPP!-->
        </Element>

        <Element>
            <Term>cbc:ID</Term>
            <Name>Transaction identifier</Name>
            <Description>An identifier for the pre-award catalogue request that allows referencing the pre-award catalogue request.</Description>
            <Value type="EXAMPLE">Transaction-02</Value>
        </Element>
        
        <Element>
            <Term>cbc:IssueDate</Term>
            <Name>Issue date</Name>
            <Description>The date something was issued. YYYY-MM-DD.</Description>
            <Value type="EXAMPLE">2018-02-06</Value>
        </Element>

        <Element cardinality="0..1">
            <Term>cbc:IssueTime</Term>
            <Name>Issue time</Name>
            <Description>The time something was issued.</Description>
            <Value type="EXAMPLE">14:23:13</Value>
        </Element>
        
        <Element>
            <Term>Note</Term>
            <Name>Version</Name>
            <Description>An identifier for the version of a transaction message.</Description>
            <Value type="EXAMPLE">Version 3 is OK</Value>
        </Element>

        <Element>
            <Term>cac:ReceiverParty</Term>
            <Name>Economic operator</Name>
            <Description>The contracting authority or contracting entity who is buying supplies, services or public
                works using a tendering procedure as described in the applicable directive.</Description>
            <Property key="doc">structure/docs/ReceiverParty-Request.adoc</Property>

            <Element cardinality="0..1">
                <Term>cbc:EndpointID</Term>
                <Name>Economic operator electronic address identifier</Name>
                <Description>Electronic address of the economic operator.</Description>
                <Value type="EXAMPLE">64372999901</Value>
            </Element>

            <Element>
                <Term>cac:PartyIdentification</Term>
                
                <Element cardinality="0..1">
                    <Term>cbc:ID</Term>
                    <Name>Economic operator identifier</Name>
                    <Description>An identifier that identifies the economic operator, such as a legal registration identifier.</Description>
                    <Value type="EXAMPLE">12324</Value>
                </Element>
            </Element>

            <Element>
                <Term>cac:PartyName</Term>

                <Element>
                    <Term>cbc:Name</Term>
                    <Name>Economic operator name</Name>
                    <Description>The name of the economic operator</Description>
                    <Value type="EXAMPLE">Supplier ltd</Value>
                </Element>
            </Element>

            <Element cardinality="0..1">
                <Term>cac:PostAddress</Term>
                <Name>Postal address</Name>

                <Element>
                    <Term>cbc:CityName</Term>
                    <Name>City</Name>
                    <Description>The common name of a city where the address is located.</Description>
                    <Value type="EXAMPLE">Oslo</Value>
                </Element>

                <Element>
                    <Term>cbc:PostalZone</Term>
                    <Name>Post code</Name>
                    <Description>The identifier for an addressable group of properties according to the relevant
                        postal service, such as a ZIP code or post code.</Description>
                    <Value type="EXAMPLE">0948</Value>
                </Element>

                <Element cardinality="0..1">
                    <Term>cbc:CountrySubentityCode</Term>
                    <Name>Country subdivision code</Name>
                    <Description>A code that identifies a subdivision of a country, e.g., a province or district.</Description>
                </Element>

                <Element> <!-- Better to use UBL CODE streetname and additionalstreetname ? -->
                    <Term>cac:AddressLine</Term>
                    
                    <Element>
                        <Term>cbc:Line</Term>
                        <Name>Address line </Name>
                        <Description>The main line in an address.</Description>
                        <Value type="EXAMPLE">Heimdalsgate 3</Value>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cbc:Line</Term>
                        <Name>Address line 2</Name>
                        <Description>An additional address line in an address that can be used to give further details
                            supplementing the main line.</Description>
                    </Element>
                </Element>

                <Element>
                    <Term>cac:Country</Term>
                    <Name>Country</Name>

                    <Element>
                        <Term>cbc:IdentificationCode</Term>
                        <Name>Country Code</Name>
                        <Description>A code that identifies the country.</Description>
                        <Value type="EXAMPLE">DK</Value>
                    </Element>

                </Element>
            </Element>

            <Element>
                <Term>cac:PhysicalLocation</Term>
                <Name>Country of registration</Name> <!-- Change name -->
                <Description>The country where the party is registered.</Description>

                <Element>
                    <Term>cbc:LocationTypeCode</Term>
                    <Name>Location code</Name> <!-- Change name -->
                    <Description>A code signifying the type of location. The code can be found in the code list.</Description>
                    <Reference type="CODE_LIST">IS03166-1_Alpha</Reference> <!-- Country_of_registation -->
                </Element>

                <Element>
                    <Term>cac:Address</Term>
                    <Name>Address</Name>

                    <Element>
                        <Term>cac:Country</Term>
                        <Name>Country</Name>
                        <Description>The country in which this address is located.</Description>

                        <Element cardinality="0..1">
                            <Term>cbc:IdentificationCode</Term>
                            <Name>Identification code</Name> <!-- Change name -->
                            <Description>A code signifying this country.</Description>
                            <Value type="EXAMPLE">UK</Value>
                        </Element>
                    </Element>

                </Element>
            </Element>

            <Element>
                <Term>cac:Contact</Term>
                <Name>Contact person</Name>

                <Element> <!-- This element is added by myself, do you agree? -->
                    <Term>cbc:Name</Term>
                    <Name>Contact name</Name>
                    <Description>The contact point's name.</Description>
                    <Value type="EXAMPLE">Ola Nordmann</Value>
                </Element>
                
                <Element cardinality="0..1">
                    <Term>cbc:Telephone</Term>
                    <Name>Contact telephone number</Name>
                    <Description>A phone number for the contact point.</Description>
                    <Value type="EXAMPLE">94384329</Value>
                </Element>

                <Element> <!-- What is the point with this element? -->
                    <Term>cbc:ElectronicMail</Term>
                    <Name>Contact point</Name>
                    <Description>An e-mail address for the contact point.</Description>
                    <Value type="EXAMPLE">ola@gmail.no</Value>
                </Element>

                <Element>
                    <Term>cbc:ElectronicMail</Term>
                    <Name>Contact email address</Name>
                    <Description>An e-mail address for the contact point.</Description>
                    <Value type="EXAMPLE">ola.nordmann@supplier.no</Value>
                </Element>
            </Element>

        </Element>

        <Element>
            <Term>cac:ProviderParty</Term>
            <Name>Contracting body</Name>
            <Description>The contracting authority or contracting entity who is buying supplies, services or public 
                works using a tendering procedure as described in the applicable directive </Description>
            
            <Element>
                <Term>cbc:EndpointID</Term>
                <Name>Contracting body electronic address identifier</Name>
                <Description>Electronic address of the contracting body.</Description>
                <Value type="EXAMPLE">74300020004</Value>
            </Element>

            <Element>
                <Term>cac:PartyIdentification</Term>

                <Element cardinality="0..1">
                    <Term>cbc:ID</Term>
                    <Name>Contracting body identifier</Name>
                    <Description>The national identifier of a contracting body as it is legally registered
                        (e.g. VAT identification, such as KBO).</Description>
                    <Value type="EXAMPLE">12345</Value>
                </Element>
            </Element>

            <Element>
                <Term>cac:PartyName</Term>
                <Name>Economic operator name</Name>
                <Description>The name of the economic operator.</Description>

                <Element>
                    <Term>cbc:Name</Term>
                    <Name>Contracting body name</Name>
                    <Description>The name of the contracting body as it is registered.</Description>
                    <Value type="EXAMPLE">David Jones</Value>
                </Element>
            </Element>

            <Element cardinality="0..1">
                <Term>cac:PostAddress</Term>
                <Name>Postal address</Name>

                <Element>
                    <Term>cbc:CityName</Term>
                    <Name>City</Name>
                    <Description>The common name of a city where the address is located.</Description>
                    <Value type="EXAMPLE">London</Value>
                </Element>

                <Element>
                    <Term>cbc:PostalZone</Term>
                    <Name>Post code</Name>
                    <Description>The identifier for an addressable group of properties according to the relevant
                        postal service, such as a ZIP code or post code.</Description>
                    <Value type="EXAMPLE">AB119QN</Value>
                </Element>

                <Element cardinality="0..1">
                    <Term>cbc:CountrySubentityCode</Term>
                    <Name>Country subdivision code</Name>
                    <Description>A code that identifies a subdivision of a country, e.,g., a province or district.</Description>
                    <Value type="EXAMPLE">Bolton</Value>
                </Element>

                <Element> <!-- Better to use UBL CODE streetname and additionalstreetname ? -->
                    <Term>cac:AddressLine</Term>

                    <Element>
                        <Term>cbc:Line</Term>
                        <Name>Address line </Name>
                        <Description>The main line in an address.</Description>
                        <Value type="EXAMPLE">Main street 3</Value>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cbc:Line</Term>
                        <Name>Address line 2</Name>
                        <Description>An additional address line in an address that can be used to give further details
                            supplementing the main line</Description>
                        <Value type="EXAMPLE">Building 32</Value>
                    </Element>
                </Element>

                <Element>
                    <Term>cac:Country</Term>
                    <Name>Country</Name>

                    <Element>
                        <Term>cbc:Identification</Term>
                        <Name>Country Code</Name>
                        <Description>A code that identifies the country.</Description>
                        <Value type="EXAMPLE">UK</Value>
                    </Element>
                </Element>
            </Element>

            <Element>
                <Term>cac:PhysicalLocation</Term>
                <Name>Country of registration</Name> <!-- Change name -->
                <Description>The country where the party is registered.</Description>

                <Element>
                    <Term>cbc:LocationTypeCode</Term>
                    <Name>Location code</Name> <!-- Change name  -->
                    <Description>A code signifying the type of location. The code can be found in the code list.</Description>
                    <Reference type="CODE_LIST">IS03166-1_Alpha</Reference> <!-- Country_of_registation -->
                </Element>

                <Element>
                    <Term>cac:Address</Term>
                    <Name>Address</Name>

                    <Element>
                        <Term>cac:Country</Term>
                        <Name>Country</Name>
                        <Description>The country in which this address is located.</Description>

                        <Element cardinality="0..1">
                            <Term>cbc:IdentificationCode</Term>
                            <Name>Identification code</Name> <!-- Change name  -->
                            <Description>A code signifying this country.</Description>
                            <Value type="EXAMPLE">123424</Value>
                        </Element>
                    </Element>

                </Element>
            </Element>

            <Element>
                <Term>cac:Contact</Term>
                <Name>Contact person</Name>

                <Element> <!-- Just added this element, do you agree  ? -->
                    <Term>cbc:Name</Term>
                    <Name>Contact name</Name>
                    <Description>The contact point's name.</Description>
                    <Value type="EXAMPLE">Birgitt Nielsen</Value>
                </Element>

                <Element cardinality="0..1">
                    <Term>cbc:Telephone</Term>
                    <Name>Contact telephone number</Name>
                    <Description>A phone number for the contact point.</Description>
                    <Value type="EXAMPLE">84737223</Value>
                </Element>

                <Element> <!-- What is point with this element ? -->
                    <Term>cbc:ElectronicMail</Term>
                    <Name>Contact point</Name>
                    <Description>An e-mail address for the contact point.</Description>
                    <Value type="EXAMPLE">birgitt@ygh.com</Value>
                </Element>

                <Element>
                    <Term>cbc:ElectronicMail</Term>
                    <Name>Contact email address</Name>
                    <Description>An e-mail address for the contact point.</Description>
                    <Value type="EXAMPLE">birgitt.nielsen@gmail.com</Value>
                </Element>
            </Element>

        </Element>

        <Element cardinality="0..1"> <!-- Need more input -->
            <Term>cac:RequestedCatalogueReference</Term>
            <Name>Previous version reference</Name>
            <Description>A reference to the previous version of a transaction message.</Description>
        </Element>

        <Element>  <!-- Need more input -->
            <Term>cac:ReferencedContract</Term>
            <Name>Call for tenders reference</Name>
            <Description>A reference to the call for tender as issued by the contracting body. Creates a link between
                the call for tender and the tender.</Description>
        </Element>
        
        <Element cardinality="1..n">
            <Term>cac:CatalogueRequestLine</Term>
            <Name>Catalogue request line</Name>
            <Description>A line in a pre-award cataloue request specifying</Description>
            
            <Element>
                <Term>cbc:ID</Term>
                <Name>Catalogue request line identifier</Name>
                <Description>An identifier for the catalogue request line being unique within pre-award catalogue request.</Description>
                <Value type="EXAMPLE">73635</Value>
            </Element>

            <Element>
                <Term>cac:RequiredItemLocationQuantity</Term>
                <Name>Required Item location quantity</Name>

                <Element> <!-- Not sure with the implementation. Need QA. -->
                    <Term>cac:Price</Term>
                    <Name>Price</Name>
                    
                    <Element>
                        <Term>cbc:PriceAmount</Term>
                        <Name>Price amount</Name>
                    </Element>

                    <Element>
                        <Term>cbc:PriceTypeCode</Term>
                        <Name>Estimated value</Name>
                        <Description>The estimated value of the an item or an requested item.</Description>
                        <Reference type="CODE_LIST">Estimated_value</Reference>
                    </Element>

                    <Element>
                        <Term>cbc:PriceTypeCode</Term>
                        <Name>Total budget amount</Name>
                        <Description>The total budget amount for the requested item.</Description>
                        <Reference type="CODE_LIST">Total_budget_amount</Reference>
                    </Element>

                </Element>
            </Element>


            <Element cardinality="0..n">
                <Term>cac:item</Term>
                <Name>Item request</Name>

                <Element cardinality="0..1">
                    <Term>cbc:Description</Term>
                    <Name>Item request description</Name>
                    <Description>A description of the item request specifying the kind of item that is requested.</Description>
                    <Value type="EXAMPLE">John Eddison need a samsung computer.</Value>
                </Element>

                <Element>
                    <Term>cbc:Name</Term>
                    <Name>Item request name</Name>
                    <Description>A name for the item request, e.g., the kind of item that is requested.</Description>
                    <Value type="EXAMPLE">Samsung computer</Value>
                </Element>
                
                <Element>
                    <Term>cac:CatalogueDocumentReference</Term>
                    <Name>Catalogue document reference</Name>

                    <Element>
                        <Term>cac:Attachment</Term>
                        <Name>Attachment document</Name>

                        <Element cardinality="0..1">
                            <Term>cac:ExternalReference</Term>
                            <Name>External reference</Name>

                            <Element cardinality="0..1">
                                <Term>cbc:URI</Term>
                                <Name>Call for tenders award criterion reference</Name>
                                <Description>A reference to the awarding criterion that the tender fulfills.</Description>
                            </Element>
                        </Element>
                    </Element>
                </Element>

                <Element>
                    <Term>cac:ItemSpecificationDocumentReference</Term>
                    <Name>Item specification document reference</Name>

                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Document identifier</Name>
                        <Description>Identifier of a document</Description>
                        <Value type="EXAMPLE">453</Value>
                    </Element>
                    
                    <Element cardinality="0..1">
                        <Term>cbc:DocumentDescription</Term>
                        <Name>Document description</Name>
                        <Description>Textual description of the document.</Description>
                        <Value type="EXAMPLE">The computer need to be 15.</Value>
                    </Element>

                    <Element>
                        <Term>cac:Attachment</Term>
                        <Name>Attachment</Name>
                        
                        <Element>
                            <Term>cac:ExternalReference</Term>
                            <Name>External reference</Name>
                            
                            <Element>
                                <Term>cbc:URI</Term>
                                <Name>External document URI</Name>
                                <Description>The Uniform Resource Identifier (URI) that identifies where the external document is located.</Description>
                                <Value type="EXAMPLE">www.elkjop.np/computer/samsung</Value>
                            </Element>
                        </Element>
                    </Element>

                    <Element>
                        <Term>cac:ResultOfVerification</Term>
                        <Name>Result of verification</Name>

                        <Element>
                            <Term>cbc:ValidationResultCode</Term>
                            <Name>Document digest</Name>
                            <Description>Digest of the document.</Description>
                            <Value type="EXAMPLE">Includes all specifications of the item.</Value>
                        </Element>

                        <Element>
                            <Term>cbc:ValidateProcess</Term>
                            <Name>Document digest method code</Name>
                            <Description>Code that indicates the algorithm used to calculate the hash.</Description>
                        </Element>
                    </Element>

                </Element>

                <Element>
                    <Term>cac:CommodityClassification</Term>
                    <Name>Commodity classification</Name>
                    <Description>A classification of this item according to a specific system for classifying commodities.</Description>

                    <Element>
                        <Term>cbc:ItemClassificationCode</Term>
                        <Name>Item request classification code</Name>

                        <Attribute>
                            <Term>Name</Term>
                            <Name>Item request classification name</Name>
                            <Description>The name of the classification for this item.</Description>
                        </Attribute>
                    </Element>
                </Element>
                
                <Element> <!-- Not sure with the implementation. Need QA. -->
                    <Term>cac:AdditionalItemProperty</Term>
                    <Name>Additional item property</Name>
                    
                    <Element>
                        <Term>cbc:ID</Term>
                        <Name>Information request identifier</Name>
                        <Description>An identifier of an information request that allows to reference the information request.</Description>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cbc:Name</Term>
                        <Name>Information request name</Name>
                        <Description>A name of the requested information</Description>
                        <Value type="EXAMPLE">Carolina Herrera</Value>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cbc:NameCode</Term>

                        <Attribute>
                            <Term>listSchemeURI</Term>
                            <Name>Information request reference</Name>
                            <Description>A code representing the requested information.</Description>
                        </Attribute>

                        <Attribute>
                            <Term>name</Term>
                            <Name>Item property classification code</Name>
                            <Description>Code for the item property according to a property code system</Description>
                            <Value type="EXAMPLE">1432</Value>
                        </Attribute>
                    </Element>

                    <Element>
                        <Term>cbc:TestMethod</Term>
                        <Name>Item property description</Name>
                        <Description>A description of the item property.</Description>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cbc:Value</Term>
                        <Name>Item request quantity</Name>
                        <Description>The quantity of the requested item offered in a pre-award catalogue.</Description>
                        <Reference type="CODE_LIST">Item_request_quantity</Reference>
                        <Reference type="CODE_LIST">Item_request_minimum_price</Reference>
                        <Reference type="CODE_LIST">Item_request_maximum_price</Reference>
                    </Element>

                    <Element cardinality="0..1">
                        <Term>cac:ValueQuantity</Term>
                        <Name>Item property value</Name>
                        <Description>The value of the item property.</Description>

                        <Attribute>
                            <Term>unitCode</Term>
                            <Name>Item property unit of measure</Name>
                            <Description>The unit of measure in which the property value is stated, if relevant.</Description>
                            <Reference type="CODE_LIST">Item_request_minimum_quantity</Reference>
                            <Reference type="CODE_LIST">Item_request_maximum_quantity</Reference>
                        </Attribute>
                    </Element>

                    <Element>
                        <Term>cbc:ImportanceCode</Term>
                        <Name>Information request relevance</Name>
                        <Description>Relevance of the requested information in the response document, in particular,
                            of it is mandatory or optional.</Description>
                    </Element>
                    
                    <Element>
                        <Term>cac:ItemPropertyRange</Term>
                        <Name>Item property range</Name>
                        
                        <Element cardinality="0..1">
                            <Term>cbc:MinimumValue</Term>
                            <Name>Item property minimum value</Name>
                            <Description>A value defining the upper boundary of the value range within which the actual
                                value of the item property is allowed.</Description>
                        </Element>

                        <Element cardinality="0..1">
                            <Term>cbc:MaximumValue</Term>
                            <Name>Item property maximum value</Name>
                            <Description>A value defining the upper boundary of the value range within which the actual
                                value of the item property is allowed.</Description>
                        </Element>
                    </Element>

                </Element>
            </Element>

        </Element>

    </Document>


</Syntax>